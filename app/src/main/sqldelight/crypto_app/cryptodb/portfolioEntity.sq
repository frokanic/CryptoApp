CREATE TABLE portfolioEntity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    symbol TEXT NOT NULL,
    price REAL NOT NULL,
    date TEXT NOT NULL,
    desc TEXT
);      --At what price did I buy?


getAllPortfolio:
SELECT *
FROM portfolioEntity;

insertToPortfolio:
INSERT OR REPLACE
INTO portfolioEntity
VALUES(?, ?, ?, ?, ?, ?); ---VALUES(id, name, symbol, price, date, desc);

deleteFromPortfolioByName:
DELETE FROM portfolioEntity
WHERE name = :name;